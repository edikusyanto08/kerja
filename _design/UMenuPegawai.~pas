unit UMenuPegawai;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls;

type
  TFMenuPegawai = class(TForm)
    Button1: TButton;
    Button2: TButton;
    Button3: TButton;
    Button4: TButton;
    Button5: TButton;
    procedure Button1Click(Sender: TObject);
    procedure Button2Click(Sender: TObject);
    procedure Button3Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  FMenuPegawai: TFMenuPegawai;

implementation

uses UBiodata, USKP, UAbsensi;

{$R *.dfm}

procedure TFMenuPegawai.Button1Click(Sender: TObject);
begin
  FBiodata.Show
end;

procedure TFMenuPegawai.Button2Click(Sender: TObject);
begin
  FAbsensi.show;
end;

procedure TFMenuPegawai.Button3Click(Sender: TObject);
begin
  FSKP.Show;
end;

end.
